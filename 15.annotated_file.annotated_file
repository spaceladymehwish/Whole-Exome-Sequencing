
java -jar snpEff.jar

java -jar snpEff.jar databases

java -jar snpEff.jar databases | grep -i Homo

java -jar snpEff.jar databases | grep -i Homo_sapiens

java -jar snpEff.jar download GRCh38.105

java -jar ~/Downloads/snpEff/snpEff.jar GRCh38.105 snps_only.recode.vcf > annotated_snpEff_snps.vcf

or

java -jar ~/Downloads/snpEff/snpEff.jar GRCh38.105 indels_only.recode.vcf > annotated_snpEff_indels.vcf



